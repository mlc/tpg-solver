{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2019",
    "sourceMap": true,
    "noEmit": true,
    "strict": true,
    "lib": ["ES2019", "DOM"],
    "esModuleInterop": true,
    "importHelpers": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "skipLibCheck": true
  },
  "exclude": [".yarn", "dist", "cdk.out"]
}
